<%- include('./header.ejs') %>
    <%- include('./sidebar.ejs') %>
    <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0 text-dark">Course View</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
              <li class="breadcrumb-item active">Course Display</li>
              <li class="breadcrumb-item active">View</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- Small boxes (Stat box) -->
        <div class="row">
            <div class="container-fluid">
                <table class="table table-bordered">
                    <thead>
                        <tr class="bg-info text-center col-sm-6">
                            <td colspan="7">Course View</td>
                        </tr>
                        <tr>
                            <td>Name</td>
                            <td>Email</td>
                            <td>Course</td>
                            <td>Phone</td>
                            <td>Address</td>
                            <td>Status</td>
                            <td>Comment</td>
                        </tr>
                    </thead>
                    <tbody>
                            <tr>
                                <td><%= c.name %></td>
                                <td><%= c.email %></td>
                                <td><%= c.course %></td>
                                <td><%= c.phone %></td>
                                <td><%= c.address %></td>
                                <td><%= c.status %></td>
                                <td><%= c.comment %></td>
            
                            </tr>
            
                    </tbody>
            
                </table>
            
            </div>
          
        </div>
        <!-- /.row -->
        
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
     <%- include('./footer.ejs') %>


    